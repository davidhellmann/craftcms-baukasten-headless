{
  "name": "frontend-nuxt",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run codegen --watch\" \"next dev\"",
    "build": "npm run codegen; next build",
    "start": "next start",
    "lint": "next lint",
    "graphql-schema": "./../backend-craftcms/craft graphql/print-schema > schema.graphql",
    "codegen": "graphql-codegen --require dotenv/config --config codegen.ts dotenv_config_path=.env.local",
    "upgrade-interactive": "npm-check --update",
    "ui": "npm run upgrade-interactive"
  },
  "dependencies": {
    "@next/font": "^13.4.3",
    "@tailwindcss/container-queries": "^0.1.1",
    "@vercel/og": "^0.5.6",
    "classnames": "^2.3.2",
    "framer-motion": "^10.12.16",
    "graphql": "16.6.0",
    "graphql-request": "^6.1.0",
    "next": "^13.4.3",
    "npm-check": "^6.0.1",
    "ramda": "^0.29.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tailwindcss-fluid-type": "^2.0.1",
    "tailwindcss-question-mark": "^0.1.1"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^4.0.0",
    "@graphql-codegen/client-preset": "^4.0.0",
    "@graphql-codegen/typescript": "^4.0.0",
    "@graphql-codegen/typescript-graphql-request": "^5.0.0",
    "@graphql-codegen/typescript-operations": "^4.0.0",
    "@types/graphql": "^14.5.0",
    "@types/node": "^20.2.3",
    "@types/ramda": "^0.29.1",
    "@types/react": "^18.2.7",
    "@types/react-dom": "^18.2.4",
    "autoprefixer": "^10.4.14",
    "concurrently": "^8.0.1",
    "encoding": "^0.1.13",
    "eslint": "^8.41.0",
    "eslint-config-next": "^13.4.3",
    "postcss": "^8.4.23",
    "postcss-import": "^15.1.0",
    "tailwindcss": "^3.3.2",
    "tailwindcss-debug-screens": "^2.2.1",
    "typescript": "^5.0.4"
  }
}
