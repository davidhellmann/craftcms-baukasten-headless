{
  "name": "frontend-next",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "prewrite:data": "tsc scripts/writeGlobalDataToDisk.ts --outDir scripts/dist --types node --module commonjs --target ES2017 --esModuleInterop",
    "write:data": "node ./scripts/dist/scripts/writeGlobalDataToDisk.js",
    "postwrite:data": "rm -rf ./scripts/dist",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint src",
    "upgrade-interactive": "npm-check --update",
    "ui": "npm run upgrade-interactive",
    "generate": "graphql-codegen --require dotenv/config --config codegen.json dotenv_config_path=.env.local"
  },
  "dependencies": {
    "graphql": "^16.6.0",
    "graphql-request": "^5.0.0",
    "next": "^12.3.1",
    "ramda": "^0.28.0",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.13.7",
    "@graphql-codegen/introspection": "^2.2.1",
    "@graphql-codegen/typescript": "^2.7.4",
    "@graphql-codegen/typescript-graphql-request": "^4.5.6",
    "@graphql-codegen/typescript-operations": "^2.5.4",
    "@types/graphql": "^14.5.0",
    "@types/node": "^18.11.2",
    "@types/ramda": "^0.28.17",
    "@types/react": "^18.0.21",
    "@types/react-dom": "18.0.6",
    "eslint": "^8.25.0",
    "eslint-config-next": "^12.3.1",
    "npm-check": "^6.0.1",
    "typescript": "^4.8.4"
  }
}
