{
  "name": "frontend-nuxt",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run codegen --watch\" \"next dev\"",
    "build": "npm run codegen; next build",
    "start": "next start",
    "lint": "next lint",
    "graphql-schema": "./../backend-craftcms/craft graphql/print-schema > schema.graphql",
    "codegen": "graphql-codegen --require dotenv/config --config codegen.ts dotenv_config_path=.env.local",
    "upgrade-interactive": "npm-check --update",
    "ui": "npm run upgrade-interactive"
  },
  "dependencies": {
    "@next/font": "^13.1.6",
    "@tailwindcss/container-queries": "^0.1.0",
    "@vercel/og": "^0.1.0",
    "classnames": "^2.3.2",
    "framer-motion": "^9.0.4",
    "graphql": "16.6.0",
    "graphql-request": "^5.0.0",
    "next": "^13.1.6",
    "npm-check": "^6.0.1",
    "ramda": "^0.28.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tailwindcss-fluid-type": "^2.0.1",
    "tailwindcss-question-mark": "^0.1.1"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^3.0.0",
    "@graphql-codegen/client-preset": "^2.0.0",
    "@graphql-codegen/typescript": "^3.0.0",
    "@graphql-codegen/typescript-graphql-request": "^4.5.8",
    "@graphql-codegen/typescript-operations": "^3.0.0",
    "@types/graphql": "^14.5.0",
    "@types/node": "^18.14.0",
    "@types/ramda": "^0.28.23",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "autoprefixer": "^10.4.13",
    "concurrently": "^7.6.0",
    "encoding": "^0.1.13",
    "eslint": "^8.34.0",
    "eslint-config-next": "^13.1.6",
    "postcss": "^8.4.21",
    "postcss-import": "^15.1.0",
    "tailwindcss": "^3.2.7",
    "tailwindcss-debug-screens": "^2.2.1",
    "typescript": "^4.9.5"
  }
}
