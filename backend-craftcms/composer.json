{
  "require": {
    "craftcms/ckeditor": "4.0.x-dev",
    "craftcms/cms": "5.0.0-alpha.11",
    "mmikkel/cp-field-inspect": "2.0.0-alpha.4",
    "mutation/translate": "4.0.0-beta.1",
    "nystudio107/craft-code-field": "5.0.0-beta.2",
    "nystudio107/craft-scripts": "^1.2.13",
    "nystudio107/craft-seomatic": "5.0.0-beta.2",
    "vlucas/phpdotenv": "^5.4",
    "yiisoft/yii2-redis": "^2.0.13"
  },
  "require-dev": {
    "nystudio107/craft-autocomplete": "^1.12.0",
    "roave/security-advisories": "dev-latest",
    "yiisoft/yii2-shell": "^2.0.4"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "allow-plugins": {
      "craftcms/plugin-installer": true,
      "yiisoft/yii2-composer": true,
      "treeware/plant": true
    },
    "sort-packages": true,
    "optimize-autoloader": true,
    "platform": {
      "php": "8.2.0"
    }
  },
  "scripts": {
    "craft-update": [
      "@pre-craft-update",
      "@post-craft-update"
    ],
    "create-folders": [
      "mkdir -p ./storage",
      "mkdir -p ./config/project",
      "mkdir -p ./web/cpresources",
      "mkdir -p ./web/assets"
    ],
    "pre-craft-update": [
      "@create-folders"
    ],
    "post-craft-update": [
      "Composer\\Config::disableProcessTimeout",
      "@php craft install/check && php craft clear-caches/compiled-templates || exit 0",
      "@php craft install/check && php craft clear-caches/data || exit 0",
      "@php craft install/check && php craft clear-caches/temp-files || exit 0",
      "@php craft install/check && php craft up || exit 0"
    ],
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "pre-update-cmd": "@pre-craft-update",
    "pre-install-cmd": "@pre-craft-update",
    "post-update-cmd": "@post-craft-update",
    "post-install-cmd": "@post-craft-update"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://composer.craftcms.com",
      "canonical": false
    }
  ]
}
