{
  "name": "baukasten-headless-astro",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "concurrently \"npm run codegen --watch\" \"astro dev\"",
    "start": "concurrently \"npm run codegen --watch\" \"astro dev\"",
    "build": "npm run codegen; astro build",
    "preview": "astro preview",
    "astro": "astro",
    "codegen": "graphql-codegen --require dotenv/config --config codegen.ts dotenv_config_path=.env",
    "upgrade-interactive": "npm-check --update",
    "ui": "npm run upgrade-interactive"
  },
  "dependencies": {
    "@astrojs/node": "^7.0.0",
    "@astrojs/react": "^3.0.7",
    "@astrojs/tailwind": "^5.0.3",
    "@headlessui/react": "^1.7.17",
    "@heroicons/react": "^2.0.18",
    "@rollup/plugin-graphql": "^2.0.4",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/container-queries": "^0.1.1",
    "@types/react": "^18.2.39",
    "@types/react-dom": "^18.2.17",
    "astro": "^4.0.2",
    "classnames": "^2.3.2",
    "graphql-request": "^6.1.0",
    "lodash-es": "^4.17.21",
    "puppeteer": "^21.6.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwindcss-fluid-type": "^2.0.3",
    "tailwindcss-theme-it": "^1.0.1",
    "typeface-jetbrains-mono": "^2.0.1",
    "typeface-lato": "^1.1.13",
    "typeface-merriweather": "^1.1.13"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.0",
    "@graphql-codegen/client-preset": "^4.1.0",
    "@graphql-codegen/typescript": "^4.0.1",
    "@graphql-codegen/typescript-graphql-request": "^6.0.1",
    "@graphql-codegen/typescript-operations": "^4.0.1",
    "@types/graphql": "^14.5.0",
    "@types/node": "^20.10.1",
    "concurrently": "^8.2.2",
    "esbuild": "^0.19.2",
    "esbuild-register": "^3.5.0",
    "graphql": "^16.8.1",
    "npm-check": "^6.0.1",
    "prettier": "^3.1.0",
    "prettier-plugin-astro": "^0.12.2",
    "prettier-plugin-tailwindcss": "^0.5.9",
    "tailwindcss": "^3.3.5",
    "vite": "^5.0.6"
  }
